<?xml version="1.0" encoding="ISO-8859-1" ?>
<!-- $Revision: 1.27 $ -->
<package version="1.0">
  <name>File_Passwd</name>
  <license>PHP</license>
  <summary>Manipulate many kinds of password files</summary>
  <description>
Provides methods to manipulate standard Unix, SMB server, AuthUser (.htpasswd), AuthDigest (.htdigest), CVS pserver and custom formatted password files.
  </description>
  <maintainers>
    <maintainer>
      <user>mike</user>
      <name>Michael Wallner</name>
      <email>mike@php.net</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <release>
    <version>1.0.0</version>
    <date>2004-03-16</date>
    <state>stable</state>
    <notes>
* Stable Release
* Removed File_Passwd_Common::__clone()
    </notes>
    <filelist>
      <dir name="/" baseinstalldir="File">
        <file role="php">Passwd.php</file>
        <dir name="Passwd">
          <file role="php">Common.php</file>
          <file role="php">Unix.php</file>
          <file role="php">Cvs.php</file>
          <file role="php">Smb.php</file>
          <file role="php">Authbasic.php</file>
          <file role="php">Authdigest.php</file>
          <file role="php">Custom.php</file>
        </dir>
        <dir name="tests" role="test">
          <file>testsuite.php</file>
          <file>test_file_passwd.php</file>
          <file>test_common.php</file>
          <file>test_unix.php</file>
          <file>test_smb.php</file>
          <file>test_cvs.php</file>
          <file>test_authbasic.php</file>
          <file>test_authdigest.php</file>
          <file>test_custom.php</file>
          <file>README.txt</file>
          <file>common.txt</file>
          <file>passwd.unix.txt</file>
          <file>passwd.cvs.txt</file>
          <file>passwd.smb.txt</file>
          <file>passwd.authbasic.txt</file>
          <file>passwd.authdigest.txt</file>
          <file>passwd.custom.txt</file>
        </dir>
      </dir>
    </filelist>
  </release>
  <deps>
    <dep type="pkg" rel="ge" version="0.8.6" optional="yes">Crypt_CHAP</dep>
    <dep type="php" rel="ge" version="4.3.0" optional="yes" />
  </deps>
</package>